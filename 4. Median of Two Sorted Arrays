class Solution(object):
    def findMedianSortedArrays(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: float
        """
        m=len(nums1)
        n=len(nums2)
        nums1.extend(nums2)
        nums1.sort()
        
        if (m+n)%2==1:
            return nums1[(m+n)/2]
        else:
            return (nums1[(m+n)//2-1]+nums1[(m+n)//2])/2.0

        
